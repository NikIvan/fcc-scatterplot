{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": 0,
        "no-use-before-define": ["error", { "functions": false }],
        "import/no-extraneous-dependencies": ["error", {
            "devDependencies": true
        }]
    }
}
