{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prefer-default-export": 0,
        "no-use-before-define": ["error", { "functions": false }]
    }
}
